    "prepublishOnly": "yarn clean-files && yarn copy-files",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write src/**/*.{ts,tsx}",
